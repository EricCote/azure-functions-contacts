        "methods": [
          "get",
          "post",
          "put",
          "delete"
        ],
        "route": "contacts/{*id}"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      },
      {
        "type": "webPubSubConnection",
        "name": "actions",
        "hub": "notification",
        "direction": "out"
      }

    ]
  }